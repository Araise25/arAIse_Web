[
  {
    "id": "arise-cli",
    "title": "arAIse CLI",
    "description": "A retro-style command-line interface",
    "icon": "Briefcase",
    "tags": ["Python", "CLI", "Retro"],
    "githubUrl": "https://github.com/arAIse/arAIse-cli",
    "videoUrl": "/videos/arise-cli-demo.mp4",
    "docsUrl": "https://docs.araise.io/cli",
    "lastUpdated": "2025-03-15",
    "longDescription": "arAIse CLI is a retro-style command-line interface inspired by the 'Solo Leveling' anime. It features ASCII art, interactive commands, and a nostalgic terminal experience.",
    "features": [
      "Interactive terminal with retro aesthetics",
      "ASCII art and animations",
      "Command history and tab completion",
      "Customizable themes and colors"
    ],
    "installCommands": [
      {
        "os": "windows",
        "command": "$script = Invoke-WebRequest -UseBasicParsing -Uri \"https://raw.githubusercontent.com/Araise25/arAIse_PM/main/windows/install.ps1\"; $script.Content | Out-File -FilePath \"$env:TEMP\\araise_install.ps1\"; & \"$env:TEMP\\araise_install.ps1\"",
        "description": "Install arAIse CLI on Windows"
      },
      {
        "os": "mac",
        "command": "curl -fsSL https://raw.githubusercontent.com/Araise25/arAIse_PM/main/unix/install.sh | bash",
        "description": "Install arAIse CLI on macOS"
      },
      {
        "os": "linux",
        "command": "curl -fsSL https://raw.githubusercontent.com/Araise25/arAIse_PM/main/unix/install.sh | bash",
        "description": "Install arAIse CLI on Linux"
      }
    ]
  },
  {
    "id": "arise-web",
