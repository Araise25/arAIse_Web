    "docsUrl": "https://docs.araise.io/web",
    "lastUpdated": "2025-03-17",
    "longDescription": "arAIse Web is a web-based platform that brings the retro terminal experience to the browser. It features a responsive design, interactive UI elements, and seamless integration with the CLI version.",
    "features": [
      "Responsive design with mobile support",
      "CRT and scanline effects for authentic retro feel",
      "Dark mode by default with customizable themes",
      "Interactive games playable in the browser",
      "Seamless switching between web and terminal modes"
    ],
    "installCommands": [
      {
        "os": "windows",
        "command": "npm install -g araise-web",
        "description": "Install arAIse Web globally on Windows"
      },
      {
        "os": "mac",
        "command": "npm install -g araise-web",
        "description": "Install arAIse Web globally on macOS"
      },
      {
        "os": "linux",
        "command": "npm install -g araise-web",
        "description": "Install arAIse Web globally on Linux"
      }
    ]
  }
